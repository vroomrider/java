package com.java.practice.casting.model;

/**
 * 
 * @author pradeep_ramesh
 *
 */
public class LionSubClass extends AnimalSuperClass {

	public void speak() {
		System.out.println("Lion roars.. ");
		printInConsole("Using StaticMethod in speak()");
		jump();
	}

	public void hunt() {
		System.out.println("Lions hunt other animals.. ");
	}

	public static void eat() {
		printInConsole("Eat..");
	}

	public static void eat(String animalFood) {
		printInConsole("Eating " + animalFood);
	}

	/**
	 * @Printing.Logs.In.Console
	 * @param str
	 */
	private static void printInConsole(String str) {
		// System.out.println("SubClass Lion Object");
		System.out.println(str);
	}
}
