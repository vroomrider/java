package com.java.practice.casting.model;

/**
 * 
 * @author pradeep_ramesh
 *
 */
public class DogSubClass extends AnimalSuperClass {

	public void speak() {
		System.out.println("Dog barks.. ");
		printInConsole("Using StaticMethod in speak()");
	}

	public void run() {
		System.out.println("Dogs run fast.. ");
	}

	public static void eat() {
		printInConsole("Eat..");
	}

	public static void eat(String animalFood) {
		printInConsole("Eating " + animalFood);
	}

	/**
	 * @Printing.Logs.In.Console
	 * @param str
	 */
	public static void printInConsole(String str) {
		// System.out.println("SubClass Dog Object");
		System.out.println(str);
	}
}
