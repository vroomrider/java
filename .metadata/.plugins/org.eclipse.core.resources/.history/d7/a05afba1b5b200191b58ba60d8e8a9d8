package com.java.practice.casting.model;

/**
 * @features SuperClass Object
 * 
 * @author pradeep_ramesh
 *
 */
public class AnimalSuperClass {

	public void public_speak() {
		printInConsole("Speaks..");
	}

	public void public_jump() {
		printInConsole("All Animals cannot Jump");
	}

	public static void public_static_eat() {
		printInConsole("public.static.Eat");
	}

	/**
	 * @Printing.Logs.In.Console
	 * @param str
	 */
	private static void printInConsole(String str) {
		// System.out.println("SuperClass Object");
		System.out.println("Animal.SuperClass." + str);
	}
}
